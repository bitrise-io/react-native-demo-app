{
  "name": "BitriseReactNativeSample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test-ci": "jest --ci",
    "lint": "eslint .",
    "postinstall": "react-native-inhibit-warnings"
  },
  "dependencies": {
    "jest-junit": "^15.0.0",
    "react": "18.2.0",
    "react-native": "0.71.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^29.4.1",
    "eslint": "^7.32.0",
    "jest": "^29.4.1",
    "metro-react-native-babel-preset": "0.72.3",
    "react-native-inhibit-warnings": "^1.0.1",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "<rootDir>/reports",
          "outputName": "test-report-junit.xml"
        }
      ]
    ]
  }
}
